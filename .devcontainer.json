{
    "name": "rmtld3synth-dev",
    "image": "docker.io/anmaped/rmtld3synth:v0.6-x",
    "workspaceFolder": "/workspaces",
    "workspaceMount": "source=${localWorkspaceFolder},target=/workspaces,type=bind,Z",
    "postCreateCommand": "uname -a && opam install ocaml-lsp-server ocamlformat -y",
    "extensions": ["ocamllabs.ocaml-platform"],
}